
# Absolute path to DATA_COLLECTOR_BASE_PACKAGE.
DATA_MODULES_PATH: /ClimateChangeApp/code/data_gathering_subsystem/data_modules/

# Absolute path to the API service root directory.
ROOT_DATA_GATHERING_SUBSYSTEM_FOLDER: /ClimateChangeApp/code/data_gathering_subsystem/

# Absolute path to the Data Gathering Subsystem root directory.
