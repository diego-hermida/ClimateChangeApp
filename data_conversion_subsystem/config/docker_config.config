
# Absolute path to DATA_COLLECTOR_BASE_PACKAGE.
DATA_CONVERTERS_PATH: /ClimateChangeApp/code/data_conversion_subsystem/data_converters/

# Absolute path to the API service root directory.
ROOT_DATA_CONVERSION_SUBSYSTEM_FOLDER: /ClimateChangeApp/code/data_conversion_subsystem/

# Absolute path to the Data Conversion Subsystem root directory.
# The "_{ID}" wildcard will be replaced at execution time, or removed if the SUBSYSTEM_INSTANCE_ID equals 1.
